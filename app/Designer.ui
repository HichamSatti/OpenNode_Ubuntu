/* QComboBox gets the &quot;on&quot; state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
background-color: qlineargradient(spread:pad, x1:0.603, y1:0.914773, x2:0.221591, y2:0.222, stop:0.323864 rgba(238, 238, 105, 255), stop:0.926136 rgba(244, 244, 160, 255));
